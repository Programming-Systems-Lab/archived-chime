  MESHFACT 'tunnel' (
    PLUGIN ('thingFact')
    PARAMS (
    CURVECENTER (0,0,0)
    CURVESCALE (80)

    CURVECONTROL ( 5, 2, 0:1  ,0)
    CURVECONTROL ( 5, 2, 5:0.5,0)
    CURVECONTROL ( 0, 2, 5:0  ,0)
    CURVECONTROL (-5, 2, 5:0.5,0)
    CURVECONTROL (-5, 2, 0:1  ,0)
